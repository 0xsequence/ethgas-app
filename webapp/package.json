  "scripts": {
    "start": "yarn dev",
    "dev": "webpack-dev-server --mode development --progress --color --config config/webpack/webpack.config.local.js",
    "dist": "yarn dist:clean && yarn dist:build",
    "dist:build": "webpack --mode production --color --config config/webpack/webpack.config.dist.js",
    "dist:clean": "rimraf ./dist",
    "dist:stats": "serve -l 5555 ./dist/.stats",
    "lint": "eslint -c .eslintrc.js './src/**/*.{ts,tsx}'",
    "lint:fix": "eslint -c .eslintrc.js --fix './src/**/*.{ts,tsx}'",
    "test:watch": "jest --watchAll",
    "test": "jest --ci --runInBand",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "fix": "yarn lint:fix && yarn format"
  },
  "dependencies": {
